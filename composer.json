{
  "name": "vendera-trading-company/laravel",
  "description": "Laravel Extension",
  "authors": [
    {
      "name": "Faruk Ert√ºrk",
      "email": "ertuerk.faruk.business@gmail.com"
    }
  ],
  "license": "MIT",
  "require": {
    "php": "^8.0",
    "livewire/livewire": "^2.8.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.25"
  },
  "autoload": {
    "psr-4": {
      "VenderaTradingCompany\\": "src"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "VenderaTradingCompany\\App\\Providers\\VenderaTradingCompanyProvider"
      ],
      "aliases": {
        "Ip": "VenderaTradingCompany\\App\\Facades\\Ip",
        "SMS": "VenderaTradingCompany\\App\\Facades\\SMS",
        "Address": "VenderaTradingCompany\\App\\Facades\\Address"
      }
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
